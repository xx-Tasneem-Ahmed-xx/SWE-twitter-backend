{
  "compilerOptions": {
    "target": "ES2020",
   
    "module": "ESNext",

    "rootDirs": ["./src", "./prisma"],
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}